SELECT *FROM winstons_donut_logs WHERE donuts_eaten >4 ORDER BY years_old;

/*AND*/
SELECT *FROM winstons_donut_logs WHERE donuts_eaten >4 AND donuts_eaten<7;

/*OR*/
SELECT *FROM winstons_donut_logs WHERE donuts_eaten >4 OR years_old <3;

SELECT SUM(donuts_eaten) AS avg_eaten FROM winstons_donut_logs
GROUP BY donuts_eaten
HAVING avg_eaten >25

/*COUNT*/
SELECT COUNT(*),
CASE
WHEN years_old > 3-1 THEN "ABOVE MAX";
WHEN years_old > ROUND(0.01 * (3-1)) THEN "ABOVE TARGET";
WHEN years_old > ROUND(0.50 *(3-1))THEN "WITHIN TARGET";
ELSE "BELOW TARGET";
END AS "donuts_eaten";
FROM winstons_donut_logs;
GROUP BY donuts_eaten;









